<h1>ログイン画面</h1>

<%= link_to '一覧', admin_users_path, class: 'nav-link' %>

<%= form_with scope: :session, local: true do |f| %>
  <div class="form-group">
      <%= f.label :email %>
      <%= f.text_field :email, class: 'form-control', id: 'sessions_email' %>
  </div>
  <div class="form-group">
      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control', id: 'sessions_password' %>
  </div>
  <br>
  <%= f.submit 'ログインする', class: 'btn btn-primary' %>
<% end %>